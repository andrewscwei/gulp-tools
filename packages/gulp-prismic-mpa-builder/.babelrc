{
  "retainLines": true,
  "sourceMaps": true,
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "ie >= 11"
        ]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-syntax-object-rest-spread",
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
